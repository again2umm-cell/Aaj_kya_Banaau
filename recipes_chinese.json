    {
      "name": "Fried Rice",
      "imageName": "fried_rice",
      "cuisine": "Chinese",
      "country": "China",
      "ingredients": [
        "• 2 cups Cooked Rice",
        "• Mixed vegetables",
        "• Soy sauce",
        "• Oil",
        "• Garlic",
        "• Eggs"
      ],
      "instructions": "1. Use day-old cooked rice\n2. Heat oil in wok\n3. Add garlic, stir for 30 seconds\n4. Add vegetables, stir-fry\n5. Add rice, break lumps\n6. Add soy sauce, mix well\n7. Stir-fry for 3-4 minutes\n8. Serve hot",
      "requiredIngredients": [
        "Rice"
      ],
      "regionalVariety": "General Chinese",
